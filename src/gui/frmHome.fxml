<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Button id="btnInloggen" fx:id="btnInloggen" layoutX="253.0" layoutY="206.0" mnemonicParsing="false" onAction="#btnLogin_Click" prefHeight="26.0" prefWidth="134.0" text="Inloggen" />
            <Button id="btnRegistreer" fx:id="btnRegistreer" layoutX="253.0" layoutY="246.0" mnemonicParsing="false" onAction="#btnRegistreer_Click" prefHeight="26.0" prefWidth="134.0" text="Registreren" />
            <ComboBox id="cboLanguage" fx:id="cboLanguage" layoutX="520.0" layoutY="14.0" onAction="#cboLanguage_SelectionChanged" prefHeight="25.0" prefWidth="106.0" visibleRowCount="3" />
            <ImageView id="lvFoto" fx:id="lvFoto" fitHeight="240.0" fitWidth="316.0" layoutX="162.0" layoutY="54.0" pickOnBounds="true" preserveRatio="true" />
         </children>
    </AnchorPane>
  </children>
</VBox>
